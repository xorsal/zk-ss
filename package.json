{
  "name": "zk-secret-santa",
  "version": "0.0.0",
  "description": "ZK Secret Santa - Privacy-preserving Secret Santa protocol on Aztec",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./artifacts/*": "./artifacts/*"
  },
  "scripts": {
    "clean": "rm -rf ./artifacts ./target codegenCache.json",
    "codegen": "aztec codegen target --outdir ./artifacts --force",
    "compile": "aztec compile",
    "test": "yarn test:js",
    "test:js": "vitest run",
    "test:noir": "aztec test",
    "ccc": "yarn clean && yarn compile && yarn codegen",
    "bench": "NODE_NO_WARNINGS=1 aztec-benchmark --suffix _base",
    "cli": "tsx src/cli/index.ts",
    "bundle-cli": "yarn ccc && mkdir -p cli/src/artifacts cli/src/target && cp artifacts/SecretSanta.ts cli/src/artifacts/ && cp target/secret_santa_contract-SecretSanta.json cli/src/target/ && cd cli && yarn install && yarn build"
  },
  "dependencies": {
    "@aztec/accounts": "3.0.0-devnet.20251212",
    "@aztec/aztec.js": "3.0.0-devnet.20251212",
    "@aztec/foundation": "3.0.0-devnet.20251212",
    "@aztec/kv-store": "3.0.0-devnet.20251212",
    "@aztec/pxe": "3.0.0-devnet.20251212",
    "@aztec/stdlib": "3.0.0-devnet.20251212",
    "@aztec/test-wallet": "3.0.0-devnet.20251212",
    "@inquirer/prompts": "^7.2.1",
    "@types/node": "22.5.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0"
  },
  "devDependencies": {
    "@defi-wonderland/aztec-benchmark": "3.0.0-devnet.2",
    "@types/jest": "29.5.11",
    "tsx": "4.20.4",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=1.22.0"
  }
}
